#include<stdio.h>
int main()
{
    float a[100],d1,d2,d3;
    int n,i,c1=0,c2=0,c3=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%f",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i]>0)
        {
            c1++;
        }
        else if(a[i]<0)
        {
            c2++;
        }
        else
        {
            c3++;
        }

    }
    d1=c1/(float)n;
    d2=c2/(float)n;
    d3=c3/(float)n;
    printf("%f\n%f\n%f",d1,d2,d3);

}
