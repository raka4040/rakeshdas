#include<stdio.h>
int main()
{
    int a[10],b[10],i,j,n1=0,n2=0,n3=0;
    for(i=0;i<3;i++)
    {
        scanf("%d",&a[i]);
        
    }
    for(i=0;i<3;i++)
    {
        scanf("%d",&b[i]);
        
    }
    for(j=0;j<3;j++)
    {
        if(a[j]>b[j])
        {
            n1=n1+1;
        }
        else if(a[j]<b[j])
        {
            n2=n2+1;
        }
        else
        {
          n3;
        }


    }
    printf("%d %d",n1,n2);
    return 0;
}
